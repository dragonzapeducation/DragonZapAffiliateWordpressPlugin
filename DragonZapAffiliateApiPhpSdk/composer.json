{
    "name": "dragonzap/affiliate-api-php-sdk",
    "description": "PHP SDK for the Dragon Zap Affiliate API",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "DragonZap\\AffiliateApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DragonZap\\AffiliateApi\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
